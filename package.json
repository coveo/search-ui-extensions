        "raw-loader": "^1.0.0",
        "rimraf": "^2.6.3",
        "sass": "^1.49.0",
        "sinon": "^7.3.2",
        "snyk": "^1.230.4",
        "ts-es5-istanbul-coverage": "^1.0.6",
        "ts-loader": "^6.0.0",
